{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@wordigo/api#build": {
      "dependsOn": [
        "@wordigo/db#build",
        "@wordigo/email#build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDotEnv": [
    ".env"
  ]
}