{
  "name": "wordigo_back",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=...api",
    "dev": "dotenv -- turbo dev --parallel --no-cache --continue --filter=...api",
    "start": "turbo run start --filter=...api",
    "lint": "turbo run lint",
    "clean": "git clean -xdf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:generate": "cd ./packages/db && npx prisma generate"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "npm@9.5.0",
  "workspaces": [
    "apps/**",
    "packages/*"
  ]
}