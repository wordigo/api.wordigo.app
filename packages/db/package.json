{
  "name": "@wordigo/db",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts && prisma generate",
    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "tsup": "^7.2.0"
  },
  "devDependencies": {
    "prisma": "^5.1.1"
  }
}